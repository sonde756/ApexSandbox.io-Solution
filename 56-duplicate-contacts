public Boolean duplicateContacts(Contact c1, Contact c2) {
    

    if((c1.email != null && c2.email != null) && (c1.email == c2.email)) return true;
    if((c1.phone != null && c2.phone != null) && (c1.phone == c2.phone)) return true;
    if((c1.email != null && c2.email != null) && (c1.email == c2.email)) return true;

    return false;
}