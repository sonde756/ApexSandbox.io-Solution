public Integer maxConsecutiveOnes(Integer[] numbers) {

   Integer count = 0;

   Integer largest = 0;

    if(Integer i = 0; i < numbers.lenght; i++){
        
        if(numbers[i] == 1){
           count++;            
        {else {
            largest = Math.max(largest, count)
        }

    }
        largest = Math.max(largest, count);

    return largest;
}