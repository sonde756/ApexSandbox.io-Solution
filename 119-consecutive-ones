public Integer maxConsecutiveOnes(Integer[] numbers) {

   Integer count = 0;

   Integer largest = 0;

    for(Integer i = 0; i < numbers.size(); i++){
        
        if(numbers[i] == 1){
           count++;            
        }else {
            largest = Math.max(largest, count);
            count = 0;
        }

    }
        largest = Math.max(largest, count);

    return largest;
}